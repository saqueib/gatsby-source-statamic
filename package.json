{
  "name": "gatsby-source-statamic-g4",
  "version": "1.0.1",
  "description": "Gatsby plugin for sourcing data from Statamic CMS",
  "author": "Saqueib <saquibweb@gmail.com>",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
