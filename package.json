    "babel-preset-gatsby-package": "^0.4.0",
    "cross-env": "^5.2.1",
    "jest": "^25.5.3",
    "standard-version": "^8.0.1"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saqueib/gatsby-source-statamic"
  },
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^2.2.0 || ^4.14.1"
  },
  "bugs": {
    "url": "https://github.com/saqueib/gatsby-source-statamic/issues"
  },
  "engines": {
    "node": ">=10.13.0"
